@article{muranaka_5-hydroxymethylfurfural_2020,
	title = {5-{Hydroxymethylfurfural} {Synthesis} from {Monosaccharides} by a {Biphasic} {Reaction}–{Extraction} {System} {Using} a {Microreactor} and {Extractor}},
	volume = {5},
	url = {https://doi.org/10.1021/acsomega.0c00399},
	doi = {10.1021/acsomega.0c00399},
	abstract = {5-Hydroxymethylfurfural (HMF) was synthesized from monosaccharides by a biphasic reaction system using a microreactor. The biphasic reaction system realized an immediate extraction and stabilization of product HMF, which further degrades under the reaction conditions. Segmented flow was utilized for an efficient reaction–extraction tool. The effect of extraction ability was evaluated based on the extraction phase/reaction phase partition coefficient of HMF. A Lewis acid catalyst was introduced to overcome the obstacle of the reaction, which was clarified as the isomerization of glucose to fructose, and improved the HMF yield to 85 mol \% under the condition of T = 180 °C and τ = 47 min. The recovery of the product HMF was also examined using a constructed microextraction system, and HMF was selectively recovered from the extraction phase.},
	number = {16},
	urldate = {2022-12-10},
	journal = {ACS Omega},
	author = {Muranaka, Yosuke and Matsubara, Kenta and Maki, Taisuke and Asano, Shusaku and Nakagawa, Hiroyuki and Mae, Kazuhiro},
	month = apr,
	year = {2020},
	pages = {9384--9390},
}

